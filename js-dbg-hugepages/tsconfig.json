{
  "compilerOptions": {
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitUseStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es2018",
    "lib": ["es2018", "es2017.string", "dom", "scripthost"]
  }
}
